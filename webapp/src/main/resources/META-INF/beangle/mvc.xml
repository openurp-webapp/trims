<?xml version="1.0"?>
<mvc>
  <profile name="openurp.edu.trims" package="org.openurp.edu.trims.action">
    <action suffix="Action" defaultMethod="index"/>
    <view path="/" style="full" type="freemarker" suffix=".ftl" />
    <url path="/" suffix=""  style="seo" />
